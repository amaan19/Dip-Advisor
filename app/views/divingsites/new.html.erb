<h1>Add a New Dive Site</h1>
<%= form_for @divingsite , html: { multipart: true } do |f| %>
  <%= f.label "Name" %>
  <%= f.text_field :name %>
  <%= f.label "Max Depth" %>
  <%= f.text_field :max_depth, placeholder: "in feet" %>
  <%= f.collection_select :country_id, Country.all, :id, :name %>
  <%= f.file_field :divesiteimage %>

  <%= f.submit "Create Diving Site"%>
<% end %>
